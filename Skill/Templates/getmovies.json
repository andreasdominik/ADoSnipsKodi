{
  "jsonrpc": "2.0",
  "method": "VideoLibrary.GetMovies",
  "params":
  {
    "limits":
    {
      "start" : 0,
      "end": 75
    },
    "properties" : ["genre", "plot", "title", "originaltitle", "year", "rating", "playcount", "file"],
    "sort":
    {
      "order": "ascending",
      "method": "label",
      "ignorearticle": true
    }
  },
  "id": "libMovies"
}
